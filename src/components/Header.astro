---
import { parseClasses } from "@/content/functions";
import GeneralSection from "./GeneralSection.astro";
interface Props {
  class?: string;
}

const props = Astro.props;
const classes = parseClasses(props.class, "flex align-middle py-2 px-3");
---

<header class={classes}>
  <GeneralSection>
    <img src="/favicon.svg" alt="Logo" class="size-8" />
  </GeneralSection>
</header>
